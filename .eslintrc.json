{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
   "rules": {
       "indent":["error", "unix"],
        "quote":[
            "error","single"
        ],
        "linebreak style":"off",
        "no-console":"off"
    }
}
